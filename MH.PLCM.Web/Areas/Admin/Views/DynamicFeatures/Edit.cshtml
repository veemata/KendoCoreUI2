@model MH.PLCM.Core.Entities.DynamicFeature

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>DynamicFeature</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="RowId" />
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataTypeId" class="control-label"></label>
                <select asp-for="DataTypeId" class="form-control"></select>
                <span asp-validation-for="DataTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LookupId" class="control-label"></label>
                <input asp-for="LookupId" class="form-control" />
                <span asp-validation-for="LookupId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ReportGroupId" class="control-label"></label>
                <input asp-for="ReportGroupId" class="form-control" />
                <span asp-validation-for="ReportGroupId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GroupOrder" class="control-label"></label>
                <input asp-for="GroupOrder" class="form-control" />
                <span asp-validation-for="GroupOrder" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DefaultValue" class="control-label"></label>
                <input asp-for="DefaultValue" class="form-control" />
                <span asp-validation-for="DefaultValue" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UnitOfMeasure" class="control-label"></label>
                <input asp-for="UnitOfMeasure" class="form-control" />
                <span asp-validation-for="UnitOfMeasure" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsBaanFeature" /> @Html.DisplayNameFor(model => model.IsBaanFeature)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="ResponsibleGroupId" class="control-label"></label>
                <input asp-for="ResponsibleGroupId" class="form-control" />
                <span asp-validation-for="ResponsibleGroupId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Active" class="control-label"></label>
                <input asp-for="Active" class="form-control" />
                <span asp-validation-for="Active" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

